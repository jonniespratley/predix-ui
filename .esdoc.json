{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
  {
    "name": "esdoc-standard-plugin",
    "option": {
      "brand": {
        "title": "predix-ui",
        "author": "Jonnie Spratley"
      },
      "manual": {
        "index": "./catalog/getting-started.md",
        "asset": "./catalog/assets",
        "files": [
          "./catalog/overview.md",
          "./catalog/installation.md",
          "./catalog/usage.md",
          "./catalog/tutorial.md",
          "./catalog/configuration.md",
          "./catalog/example.md",
          "./CHANGELOG"
        ]
      }
    }
  },
  {"name": "esdoc-react-plugin"},
  {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}]
}
