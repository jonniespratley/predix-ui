{
  "name": "predix-ui",
  "description": "px-* web components in React.js components",
  "author": "Jonnie Spratley",
  "user": "jonniespratley",
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonniespratley/predix-ui.git"
  },
  "homepage": "https://jonniespratley.github.io/predix-ui/",
  "bugs": {
    "url": "https://github.com/jonniespratley/predix-ui/issues"
  },
  "browser": "dist/predix-ui.min.js",
  "main": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "config": {
    "library": "PxReact",
    "libraryTarget": "umd"
  },
  "scripts": {
    "plop": "./node_modules/.bin/plop",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "docs": "esdoc",
    "dist:icons": "node ./tools/icons-optimize-and-fix.js",
    "start": "webpack-dev-server --env dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --cache",
    "gh-pages": "webpack --env ghPages",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --env ghPages --profile --json > stats.json",
    "dist": "./node_modules/.bin/gulp",
    "dist:es6": "rimraf ./dist/es6 && BABEL_ENV=es6 babel ./src --out-dir ./dist/es6",
    "dist:min": "webpack --env distMin",
    "dist:modules": "rimraf ./dist/modules && BABEL_ENV=modules babel ./src --out-dir ./dist/modules",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am ''",
    "prepublish": "npm run dist:es6 && npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node tools/post_install.js"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ],
  "devDependencies": {
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "catalog": "^2.5.3",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "debug": "^3.0.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "esdoc": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.2",
    "gh-pages": "^1.0.0",
    "git-prepush-hook": "^1.0.2",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-bower": "^0.0.13",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-eslint": "^4.0.0",
    "gulp-filelog": "^0.4.1",
    "gulp-help": "^1.6.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "i": "^0.3.5",
    "imports-loader": "^0.7.1",
    "jest": "^21.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "node-sass-import-once": "^1.2.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "plop": "^1.8.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^5.0.0",
    "purecss": "^1.0.0",
    "purifycss-webpack": "^0.7.0",
    "px-table-view": "https://github.com/PredixDev/px-table-view.git",
    "px-theme": "https://github.com/PredixDev/px-theme.git",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-github-corner": "^1.0.0",
    "react-test-renderer": "16",
    "react-with-styles": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.18.2",
    "styled-jsx": "^1.0.10",
    "svg-inline-loader": "^0.8.0",
    "svgo": "^0.7.2",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.8",
    "wallaby-webpack": "^0.0.42",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "webpack-stream": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup.js",
    "collectCoverage": true,
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)",
      "**/src/**/*.test.js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "scss",
      "css"
    ],
    "modulePathIgnorePatterns": [
      "bower_components",
      "plop-templates",
      "stories",
      "docs"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
