{
  "ignore": [
    "node_modules"
  ],
  "presets": [
    "react",
    "stage-0", ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "transform-decorators",
    "transform-class-properties", ["transform-react-remove-prop-types", {
      "mode": "wrap"
    }]
  ],
  "env": {
    "dev": {
      "presets": [
        "env",
        "stage-0",
        "react-hmre"
      ]
    },
    "modules": {
      "presets": [
        ["env", {
          "modules": "umd"
        }]
      ],
      "plugins": [
        "babel-plugin-styled-components",
        "transform-class-properties"
      ]
    },
    "es6": {
      "presets": [
        ["env", {
          "modules": false
        }]
      ],
      "plugins": [
        "transform-class-properties",
        "babel-plugin-styled-components"
      ]
    }
  }
}
